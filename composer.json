{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./local-packages/socialiteproviders/telegram"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "bavix/laravel-wallet": "^9.5",
        "bensampo/laravel-enum": "^6.0",
        "filament/filament": "^2.16",
        "filament/spatie-laravel-tags-plugin": "^2.0",
        "filament/tables": "^2.16",
        "guzzlehttp/guzzle": "^7.2",
        "joelbutcher/socialstream": "^3.8",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.12",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "mad-web/laravel-initializer": "^3.4",
        "nutgram/nutgram": "^3.7",
        "opcodesio/log-viewer": "^1.6",
        "socialiteproviders/telegram": "dev-main",
        "spatie/laravel-sluggable": "^3.4",
        "stancl/virtualcolumn": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "dragon-code/pretty-routes": "^3.2",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
